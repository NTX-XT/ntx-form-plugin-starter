{
    "name": "@ntx/ntx-form-plugin-starter",
    "private": true,
    "version": "1.0.0",
    "description": "A template repository for creating Nintex Form plugins",
    "type": "module",
    "scripts": {
        "start": "npx ntx-localdev -f ./build/dev",
        "build:dev": "rollup -c --environment DEV --watch",
